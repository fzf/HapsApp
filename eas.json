{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://localhost:3000",
        "EXPO_PUBLIC_BUILD_TYPE": "development"
      }
    },
    "local-dev": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://localhost:3000",
        "EXPO_PUBLIC_BUILD_TYPE": "local-dev"
      },
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://haps.app",
        "EXPO_PUBLIC_BUILD_TYPE": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://haps.app",
        "EXPO_PUBLIC_BUILD_TYPE": "production"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production-simulator": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://haps.app",
        "EXPO_PUBLIC_BUILD_TYPE": "production-simulator"
      },
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "595608",
        "appleId": "fletch@fzf.me",
        "appleTeamId": "A483478F88"
      },
      "android": {}
    }
  }
}
